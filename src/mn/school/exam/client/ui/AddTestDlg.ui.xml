<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.panel}" width="300" height="160">
		<table>
			<tr>
				<td>
					<g:Label text="Тестийн нэр" />
				</td>
				<td>
					<g:TextBox width="179px" height="17px" ui:field="tbName" />
				</td>
			</tr>
			<tr>
				<td>
					<g:RadioButton name="radioType" text="Ердийн"
						checked="true" formValue="mn.school.exam" ui:field="rbNormal" />
					<g:RadioButton width="84px" height="18px" name="radioType"
						text="Улирлын" formValue="final" ui:field="rbFinal" />
				</td>
				<td>
					<g:RadioButton width="102px" height="18px" name="radioType"
						text="Туршилтын" formValue="practice" ui:field="rbPractice" />
				</td>

			</tr>
			<tr>
				<td>
					<g:Button ui:field="btnCancel">Болих</g:Button>
				</td>
				<td>
					<g:Button ui:field="btnAdd">Нэмэх</g:Button>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>
